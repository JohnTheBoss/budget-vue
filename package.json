{
  "name": "budget-vue",
  "version": "1.0.0",
  "description": "Simple Budget app with Vue.Js",
  "author": "JohnTheBoss",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "dev": "npx cross-env NODE_ENV=development nodemon server/index.js --watch server",
