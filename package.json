    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.0.0",
    "vue-chartjs": "^3.5.0",
    "vue-i18n": "^8.15.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "cross-env": "^6.0.3",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "sass-loader": "^8.0.0"
  }
